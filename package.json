{
  "name": "timer",
  "displayName": "Work Timer",
  "description": "An extension that shows a timer in the status bar and shows information message when 8 hours is finished. Its for all who works from home and always forgets count of hours. ",
  "version": "0.0.1",
  "publisher": "Mohammad",
  "engines": {
    "vscode": "^1.60.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "timer.resetTime",
        "title": "Reset Timer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "typescript": "^4.3.5",
    "vscode-test": "^1.6.0"
  }
}
